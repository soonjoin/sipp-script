<!--                 Sipp 'uac' scenario with pcap (rtp) play           -->
<!--                                                                    -->

<scenario name="UAC with media">
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->

  <Global variables="media_duration,from_prefix,to_prefix,media_port_offset" />
  <init>
    <nop>
      <action>
        <strcmp assign_to="empty" variable="media_duration" value="" />              
        <test assign_to="empty" compare="equal" variable="empty" value="0" />                            
      </action>
    </nop>
    <nop condexec="empty">              
      <action>
        <assignstr assign_to="media_duration" value="0" />
      </action>
    </nop>
    
    <nop>
      <action>
        <strcmp assign_to="empty" variable="from_prefix" value="" />
        <test assign_to="empty" compare="equal" variable="empty" value="0" />
      </action>
    </nop>
    <nop condexec="empty">
      <action>
        <assignstr assign_to="from_prefix" value="130000" />
      </action>
    </nop>

    <nop>
      <action>
        <strcmp assign_to="empty" variable="to_prefix" value="" />
        <test assign_to="empty" compare="equal" variable="empty" value="0" />
      </action>
    </nop>
    <nop condexec="empty">
      <action>
        <assignstr assign_to="to_prefix" value="95013" />
      </action>
    </nop>

    <nop>
      <action>
        <strcmp assign_to="empty" variable="media_port_offset" value="" />
        <test assign_to="empty" compare="equal" variable="empty" value="0" />
      </action>
    </nop>
    <nop condexec="empty">
      <action>
        <assign assign_to="media_port_offset" value="0" />
      </action>
    </nop>
    <nop>
      <action>
        <todouble assign_to="media_port_offset" variable="media_port_offset" />
      </action>
    </nop>
  </init>

  <nop>
    <action>
      <assignstr assign_to="local_rtp_port" value="[field0]" />
      <todouble assign_to="local_rtp_port" variable="local_rtp_port" />
      <add assign_to="local_rtp_port" variable="media_port_offset" />
      <assignstr assign_to="local_rtp_port" value="[$local_rtp_port]" /> 
      <ereg regexp="([[:alnum:]]*).([[:alnum:]]*)" search_in="var" variable="local_rtp_port"  check_it="true" assign_to="dummy,local_rtp_port"/>
      <log  message="local_rtp_port:[$local_rtp_port]"/>
    </action>
  </nop>

  <send retrans="500">
    <![CDATA[

      INVITE sip:[$to_prefix][field0]@[remote_ip]:[remote_port] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [$from_prefix][field0] <sip:[$from_prefix][field0]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: [$to_prefix][field0] <sip:[$to_prefix][field0]@[remote_ip]:[remote_port]>
      Call-ID: [call_id]
      CSeq: 1 INVITE
      Contact: sip:[$from_prefix][field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      Content-Type: application/sdp
      Content-Length: [len]

      v=0
      o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
      s=-
      c=IN IP[local_ip_type] [local_ip]
      t=0 0
      m=audio [$local_rtp_port] RTP/[field1] [field2] 101
      a=rtpmap:101 telephone-event/8000
      a=fmtp:101 0-11,16
      [field4]
      [field5]
      [field6]
      [field7]
      [field8]
      [field9]

    ]]>
  </send>

  <Reference variables="dummy" />

  <recv response="100" optional="true">
  </recv>

  <recv response="180" optional="true">
  </recv>

  <recv response="183" optional="true">
  </recv>

  <!-- By adding rrs="true" (Record Route Sets), the route sets         -->
  <!-- are saved and used for following messages sent. Useful to test   -->
  <!-- against stateful SIP proxies/B2BUAs.                             -->
  <recv response="200" rtd="true" crlf="true" rrs="true">
    <action>
      <ereg regexp="c=IN IP4 ([[:alnum:]]*.[[:alnum:]]*.[[:alnum:]]*.[[:alnum:]]*)" search_in="msg" check_it="true" assign_to="dummy,remote_rtp_ip"/>
      <ereg regexp="m=audio ([[:alnum:]]*)" search_in="msg" check_it="true" assign_to="dummy,remote_rtp_port"/>
    </action>
  </recv>
  <Reference variables="dummy" />

  <!-- Packet lost can be simulated in any send/recv message by         -->
  <!-- by adding the 'lost = "10"'. Value can be [1-100] percent.       -->
  <send>
    <![CDATA[

      ACK [next_url] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [$from_prefix][field0] <sip:[$from_prefix][field0]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: [$to_prefix][field0] <sip:[$to_prefix][field0]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 1 ACK
      Contact: sip:[$from_prefix][field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      [routes]
      Content-Length: 0

    ]]>
  </send>

  <!-- Play a pre-recorded RTP stream(loop)                             -->
  <nop>
    <action>
      <exec command="rtp/rtpsend-sipp -l -d [$media_duration] -s [$local_rtp_port] -f rtp/payload_[field2].[field3] [$remote_rtp_ip]/[$remote_rtp_port]"/>
      <log  message="[field2],[$remote_rtp_ip],[$remote_rtp_port]"/>
    </action>
  </nop>

  <!-- Pause any time you want                                          -->
  <pause/>

  <!-- Stop play rtp stream                                             -->
  <!--<nop>
    <action>
      <exec command="rtp/killport.sh [$local_rtp_port]"/>
    </action>
  </nop>-->

  <!-- Play an out of band DTMF '1'                                     -->
  <!--
  <nop>
    <action>
      <exec play_pcap_audio="pcap/dtmf_2833_1.pcap"/>
    </action>
  </nop>
  -->

  <!-- <pause milliseconds="1000"/> -->

  <!-- The 'crlf' option inserts a blank line in the statistics report. -->
  <send retrans="500">
    <![CDATA[

      BYE [next_url] SIP/2.0
      Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
      From: [$from_prefix][field0] <sip:[$from_prefix][field0]@[local_ip]:[local_port]>;tag=[pid]SIPpTag09[call_number]
      To: [$to_prefix][field0] <sip:[$to_prefix][field0]@[remote_ip]:[remote_port]>[peer_tag_param]
      Call-ID: [call_id]
      CSeq: 2 BYE
      Contact: sip:[$from_prefix][field0]@[local_ip]:[local_port]
      Max-Forwards: 70
      Subject: Performance Test
      [routes]
      Content-Length: 0

    ]]>
  </send>

  <recv response="200" crlf="true">
  </recv>

  <!-- definition of the response time repartition table (unit is ms)   -->
  <ResponseTimeRepartition value="10, 20, 30, 40, 50, 100, 150, 200"/>

  <!-- definition of the call length repartition table (unit is ms)     -->
  <CallLengthRepartition value="10, 50, 100, 500, 1000, 5000, 10000"/>

</scenario>

